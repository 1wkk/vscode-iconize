{
  "publisher": "yurika",
  "name": "iconize",
  "displayName": "iconize",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.5.1",
  "description": "",
  "author": "Yurika Xu <iherewithmyheart@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/echoeureka/vscode-iconize#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/echoeureka/vscode-iconize"
  },
  "bugs": {
    "url": "https://github.com/echoeureka/vscode-iconize/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.16.3",
    "@babel/types": "^7.18.8",
    "@types/node": "^18.0.3",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.0",
    "vsce": "^2.9.2"
  }
